{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static",
      "config": {
        "installCommand": "npm install --no-cache",
        "buildCommand": "npm run web:build && npm run postbuild",
        "outputDirectory": "web-build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
